
class P9 {
	
	public static void main(String args []) {
		
		for (char rows = 1; rows <= 5; rows++) {
			
			for (int space = 5 - rows; space > 0; space--)
				
				System.out.print(" ");
			
			for (char column = 'A'; column <= rows; column++){
				
				System.out.print(column + " ");
				
			}
			
			System.out.println();
			
		}
		
	}	
	
}